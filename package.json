    "dts-cli": "^2.0.5",
    "husky": "^7.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^6.0.4",
    "tslib": "^2.3.1",
    "typescript": "^5.1.6"
